{
    "default": {
        "formatOptions":{
            "snippetInterface": "async-await"
        },
        "paths":[
            "src/test/features/"
            
        ],
        "publishQuiet": false,
        "dryRun": false,
        "require":[
            "src/test/steps/*.js",
            "src/hooks/hooks.js"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel":2
    },


    "rerun": {
        "formatOptions":{
            "snippetInterface": "async-await"
        },
        "publishQuiet": false,
        "dryRun": false,
        "require":[
            "src/test/steps/*.js",
            "src/hooks/hooks.js"
        ],
        "format": [
            "progress-bar",
            "html:test-results/cucumber-report.html",
            "json:test-results/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel":2
    }
}